{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "ConnectionString": "/app/db/SpaceRogue.db",
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "System": "Warning"
      }
    }
  },
  "Kestrel": {
    "http1Port": 45665,
    "http2Port": 8891
  },
  "AzureServiceBusEnabled": false,
  "SubscriptionClientName": "Galaxy",
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "EventBusRetryCount": 5,
  "EventBusConnection": "galaxy.rabbit",
  "EventBusUserName": "guest",
  "EventBusPassword": "guest"
}